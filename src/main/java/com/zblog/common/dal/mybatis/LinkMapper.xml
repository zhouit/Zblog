<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
      "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zblog.common.dal.mapper.LinkMapper">
   <select id="list" resultType="MapContainer">  
      select id, name, url, visible,lastUpdate from link
   </select>
   
   <insert id="insert" parameterType="Link">
      insert into link(id,name,url,notes,visible,creator,createTime,) 
         values(#{id},#{name},#{url},#{notes},#{visible},#{creator}),#{createTime},#{lastUpdate}))
   </insert>

   <delete id="deleteById">
	 delete from link where id = #{id}
  </delete>
	
</mapper>